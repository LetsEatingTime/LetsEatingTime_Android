<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="tv_toolbar">
        <item name="android:text">밥먹자</item>
        <item name="android:textSize">26sp</item>
        <item name="android:color">@color/black</item>
    </style>
    <style name="tv_signup_text">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="tv_signup_inp">
        <item name="android:paddingLeft">22dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:background">@null</item>
    </style>
    <style name="tv_signup_pb_text">
        <item name="android:layout_marginRight">18dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>
    <style name="btn_signup_submit">
        <item name="android:text">다음</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">22sp</item>
        <item name="android:background">@drawable/btn_submit</item>
        <item name="android:layout_width">370dp</item>
        <item name="android:layout_height">48dp</item>
    </style>
</resources>