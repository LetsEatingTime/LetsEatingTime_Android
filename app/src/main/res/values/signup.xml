<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="tv_toolbar">
        <item name="android:text">LetsEatingTime</item>
        <item name="android:textSize">26dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/pretendardmedium</item>
    </style>

    <style name="tv_signup_text">
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="tv_signup_inp">
        <item name="android:paddingLeft">22dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:background">@null</item>
    </style>

    <style name="tv_signup_pb_text">
        <item name="android:layout_marginRight">18dp</item>
        <item name="android:layout_marginBottom">3dp</item>
    </style>

    <style name="tv_hint">
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/edit_text_hit</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="btn_signup_submit">
        <item name="android:text">다음</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">22dp</item>
        <item name="android:layout_marginStart">15dp</item>
        <item name="android:layout_marginEnd">15dp</item>
        <item name="android:background">@drawable/btn_submit2</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
    </style>
</resources>